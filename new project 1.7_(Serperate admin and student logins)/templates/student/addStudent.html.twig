{% extends '_base.html.twig' %}

{% block pageTitle %}Add User Page {% endblock %}

{% block main %}
    <form
            action="/index.php?action=addStudent"
            method="post"
    >

        <p>
            username:
            <input type="text" name="username">
        </p>

        <p>
            password:
            <input type="password" name="password">
        </p>
        <p>
            role:(admin/user)
            <input type="text" name="role">

        </p>


        <input type="submit" value="login">

    </form>

{% endblock %}